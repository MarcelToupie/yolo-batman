bool estFeuille(Arbre a){
	return nbFils==0;
}
T etq(Arbre a){
	return etq;
}
Arbre renvoiFils(T etq){
	Arbre fils = NULL;
	for(int i=0; i<nbFils; i++){
		if(lesFils[i].etq) fils = tab[i]; 
	}
	
	return fils;
}
Arbre ajouterFils(T etq){
	lesFils[nbFils+1]<-new Arbre(a);
	++nbFils;
}
